const prompt = require("prompt-sync")();

const senha = 1501;
let Senha_Solicitada
do{
    Senha_Solicitada = prompt("Informe a senha: ");
        if(Senha_Solicitada !=senha){
            console.log("senha incorreta");}
        else(
            console.log("Bem vindo")
        )    
        }
while(Senha_Solicitada == senha);

